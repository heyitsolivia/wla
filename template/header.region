<!doctype html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="initial-scale=1">
    <!--Include sqs scripts, meta tags, and user content from the code injection tab.-->
    {squarespace-headers}
    <link href="assets/styles/app.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,700,400italic,700italic" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:700" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Domine" rel="stylesheet" type="text/css">
</head>
<body id="{squarespace.page-id}" class="{squarespace.page-classes}">
    <header id="header" class="header row" style="background-image: url({collection.mainImage.assetUrl});">
        <div class="large-12 columns">
            <!--Site logo-->
            {.section website}
            <h1 class="site-title" data-content-field="site-title">
                <a href="/">
                    {.section logoImageUrl}
                    <img class="logo" src="{logoImageUrl}?format=original" alt="{siteTitle}">
                    {.or}
                    <img class="logo" src="assets/images/wla-logo.svg" alt="{siteTitle}">
                    {.end}
                </a>
            </h1>
            {.end}

            <!--Main navigation.-->
            <squarespace:navigation navigationId="mainNav" template="navigation"/>
            <hr>
        </div>
    </header>