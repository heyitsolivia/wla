<!-- Component consisting of header content -->
<!-- Includes brand logo, desktop and mobile navigation, and header subscribe button -->

<!-- Header Subscribe button -->
<div class="row show-for-large-up">
    <div class="small-12 columns">
        <a class="header__button right" href="http://mailchimp.com">
            Subscribe to Our Newsletter
        </a>
    </div>
</div>

<div class="row">
    <div class="navigation-wrapper">
        <!-- Site logo -->
